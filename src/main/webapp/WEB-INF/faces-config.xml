<?xml version="1.0"?>
<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
   <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/admin/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>aktivitet</from-outcome>
            <to-view-id>/aktivitet.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>resultater</from-outcome>
            <to-view-id>/resultat.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>bilder</from-outcome>
            <to-view-id>/bilder.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>spillere</from-outcome>
            <to-view-id>/spillere.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>kontakt</from-outcome>
            <to-view-id>/kontakt.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>linker</from-outcome>
            <to-view-id>/linker.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>poeng</from-outcome>
            <to-view-id>/poengLista.xhtml</to-view-id>
            <redirect/>
        </navigation-case>

        <navigation-case>
            <from-outcome>history</from-outcome>
            <to-view-id>/historie.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>redigerNyhet</from-outcome>
            <to-view-id>/admin/redigerNyheter.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>nyNyhet</from-outcome>
            <to-view-id>/admin/nyNyhet.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>redigerAktivitet</from-outcome>
            <to-view-id>/admin/redigerAktivitet.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>nyAktivitet</from-outcome>
            <to-view-id>/admin/nyAktivitet.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>redigerResultat</from-outcome>
            <to-view-id>/admin/redigerResultat.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>nyResultat</from-outcome>
            <to-view-id>/admin/nyResultat.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>spillerKamp</from-outcome>
            <to-view-id>/admin/spiller_kamp.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>redigerSpiller</from-outcome>
            <to-view-id>/admin/redigerSpillere.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>nySpiller</from-outcome>
            <to-view-id>/admin/nySpiller.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>nyPoengKamp</from-outcome>
            <to-view-id>/admin/nyPoengKamp.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>adminKampRaport</from-outcome>
            <to-view-id>/admin/adminKamprapport.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>kampRaporter</from-outcome>
            <to-view-id>/kamprapporter.xhtml</to-view-id>
            <redirect/>
        </navigation-case>

    </navigation-rule>
    <validator>
        <validator-id>myValidator</validator-id>
        <validator-class>no.rosenhoff.servlet.MyValidator</validator-class>
    </validator>
    <application>
        <locale-config>
            <default-locale>no</default-locale>
        </locale-config>
        <variable-resolver>org.springframework.web.jsf.DelegatingVariableResolver</variable-resolver>
    </application>
</faces-config>
